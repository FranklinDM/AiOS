#------------------------------------------------------------------------------
# Primary content
#------------------------------------------------------------------------------

content     aios            content/        contentaccessible=yes
skin        aios            global          skin/
manifest    locales.manifest

overlay     chrome://browser/content/browser.xul                    chrome://aios/content/aios.xul
overlay     chrome://global/content/customizeToolbar.xul            chrome://aios/content/aios_customize.xul

#------------------------------------------------------------------------------
# Panel overlays
#------------------------------------------------------------------------------

overlay     chrome://browser/content/bookmarks/bookmarksPanel.xul   chrome://aios/content/panel/places.xul
overlay     chrome://browser/content/history/history-panel.xul      chrome://aios/content/panel/places.xul
overlay     about:addons                                            chrome://aios/content/panel/addons.xul
overlay     chrome://mozapps/content/xpinstall/xpinstallConfirm.xul chrome://aios/content/panel/addons_xpinstall.xul
